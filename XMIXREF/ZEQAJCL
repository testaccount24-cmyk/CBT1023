       *-*  then
       *-*  do
   908 *-*   LINE2A = 'ENVAR("EQA_STARTUP_KEY=DCC")'
   909 *-*   'ISREDIT LINE_AFTER ' Insert_line# ' = DATALINE (LINE2A)'
   910 *-*   Insert_line# = Insert_line# + 1
   911 *-*  end
