   960 *-*    'ISREDIT LINE_AFTER ' Insert_line#' = DATALINE (LINE)'
   961 *-*    Insert_line# = Insert_line# + 1
   962 *-*   end
   963 *-*  end
   964 *-* call Insert_comments
   965 *-* if EQALIBS = '/'
       *-*  then
       *-*  do
   966 *-*   call Add_EQADEBUG_Libraries
   967 *-*  end
