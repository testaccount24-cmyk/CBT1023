   893 *-*   LINE2 = 'TEST'
   894 *-*  end
   895 *-* otherwise
       *-*  nop
   896 *-* end
   897 *-* BELOWX = BELOW || program EQAPGM
   898 *-* "ISREDIT LINE_AFTER " Insert_line# " = DATALINE (BELOWX)"
   899 *-* Insert_line# = Insert_line# + 1
   900 *-* 'ISREDIT LINE_AFTER ' Insert_line# ' = DATALINE (LINE1)'
   901 *-* Insert_line# = Insert_line# + 1
   902 *-* if EQACC = '/'
       *-*  then
       *-*  do
   903 *-*   LINE2 = LINE2 || ','
   904 *-*  end
